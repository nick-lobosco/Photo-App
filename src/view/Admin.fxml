<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.AdminController">
   <children>
      <ListView fx:id="userListView" layoutY="40.0" prefHeight="150.0" prefWidth="200.0" />
      <TextField fx:id="newUser" layoutY="190.0" onAction="#newUser" prefHeight="25.0" prefWidth="200.0" promptText="Add New User" />
      <Label layoutX="78.0" layoutY="13.0" prefHeight="20.0" prefWidth="44.0" text="Admin">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Button layoutX="14.0" layoutY="10.0" maxHeight="-Infinity" maxWidth="-Infinity" mnemonicParsing="false" onAction="#logout" prefHeight="25.0" prefWidth="55.0" text="Logout" />
      <Button alignment="CENTER" contentDisplay="CENTER" layoutX="135.0" layoutY="10.0" mnemonicParsing="false" onAction="#quit" prefHeight="25.0" prefWidth="55.0" text="Quit" textAlignment="CENTER" />
   </children>
</AnchorPane>
